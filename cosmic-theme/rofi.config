* {
  background: rgba(34, 23, 45, 50%);
  background-color: @background;
  border-color: @background;
  text-color: #F92672;
  spacing: 0;
  width: 512px;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
  border: 0 1px 0 0;
}

textbox {
  background-color: @background;
  border: 0 0 1px 0;
  border-color: @background;
  padding: 8px 16px;
  text-color: #F92672;
}

entry {
  padding: 16px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  border: 0 0 1px 0;
  padding: 16px;
}

element selected {
  text-color: #0abdc6;
}